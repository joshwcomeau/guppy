{
  "name": "guppy",
  "productName": "Guppy",
  "version": "0.3.0",
  "private": true,
  "main": "src/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.electron.guppy",
    "files": [
      "build/**/*"
    ],
    "asarUnpack": [
      "node_modules/yarn/"
    ],
    "publish": "github"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshwcomeau/guppy.git"
  },
  "license": "ISC",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm run start:react",
    "start:react": "cross-env BROWSER=none node scripts/start.js",
    "build": "node scripts/build.js",
    "dist": "cross-env GENERATE_SOURCEMAP=false npm run build && electron-builder -c.extraMetadata.main=build/electron.js",
    "dist:all": "cross-env GENERATE_SOURCEMAP=false npm run build && electron-builder -mwl -c.extraMetadata.main=build/electron.js",
    "publish": "yarn dist --publish always",
    "publish:all": "yarn dist:all --publish always",
    "test": "node scripts/test.js --maxWorkers=2 --env=node",
    "test:dev": "yarn test --verbose=false",
    "test:coverage": "yarn test --coverage",
    "test:report-coverage": "yarn test:coverage && codecov",
    "flow": "flow",
    "lint": "eslint src/**/*.js src/**/*.snap config/**/*.js",
    "prettier": "prettier --write",
    "detect-port": "cross-env ./node_modules/detect-port-alt/bin/detect-port 3000",
    "precommit": "lint-staged",
    "prepush": "yarn run flow",
    "build-storybook": "build-storybook -c .storybook -o storybook-static",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,mjs}": [
        "npm run prettier",
        "eslint",
        "git add"
      ]
    }
  },
  "dependencies": {
    "create-react-app": "1.5.2",
    "electron-debug": "2.0.0",
    "electron-devtools-installer": "2.2.4",
    "electron-store": "2.0.0",
    "electron-updater": "3.1.2",
    "fuse.js": "3.3.0",
    "gatsby-cli": "1.1.58",
    "js-yaml": "3.12.0",
    "ps-tree": "1.2.0",
    "react-beautiful-dnd": "11.0.1",
    "react-custom-scrollbars": "4.2.1",
    "react-responsive": "6.1.1",
    "rimraf": "2.6.2",
    "yarn": "1.12.3"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-rc.1",
    "@babel/runtime": "7.0.0-beta.44",
    "@storybook/addon-actions": "3.4.10",
    "@storybook/addon-info": "3.4.10",
    "@storybook/react": "3.4.10",
    "async": "2.6.1",
    "autoprefixer": "7.2.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "22.1.0",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-named-asset-import": "1.0.0-next.66cc7a90",
    "babel-plugin-react-docgen": "2.0.0-babel7.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-react-app": "4.0.0-next.66cc7a90",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.4.1",
    "codecov": "3.1.0",
    "color": "3.0.0",
    "cross-env": "5.2.0",
    "css-loader": "0.28.9",
    "dotenv": "5.0.0",
    "dotenv-expand": "4.2.0",
    "electron": "4.0.0",
    "electron-builder": "20.31.3",
    "electron-ipc-mock": "0.0.3",
    "electron-log": "2.2.17",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.7.0",
    "eslint-config-react-app": "3.0.4",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "3.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "22.2.2",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "flow-bin": "0.83.0",
    "foreman": "2.0.0",
    "fs-extra": "5.0.0",
    "graphql": "0.12.3",
    "graphql-tag": "2.6.1",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "immer": "1.3.1",
    "import-all.macro": "2.0.3",
    "jest": "23.3",
    "jest-fetch-mock": "2.1.0",
    "jest-styled-components": "6.2.2",
    "jsdom": "13.0.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "7.3.0",
    "loader-utils": "1.1.0",
    "lolex": "3.0.0",
    "mixpanel-browser": "2.22.4",
    "moment": "2.22.2",
    "node-fetch": "2.3.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.10",
    "prettier": "1.13.7",
    "random-seed": "0.3.0",
    "react": "16.8.6",
    "react-dev-utils": "6.0.0-next.66cc7a90",
    "react-docgen": "2.21.0",
    "react-dom": "16.8.6",
    "react-icons-kit": "1.1.5",
    "react-instantsearch": "5.1.0",
    "react-motion": "0.5.2",
    "react-powerplug": "1.0.0-rc.1",
    "react-redux": "5.0.7",
    "react-resize-observer": "1.1.1",
    "react-spring": "5.3.8",
    "react-test-renderer": "16.6.0",
    "react-tippy": "1.2.2",
    "react-transition-group": "2.3.1",
    "redux": "4.0.0",
    "redux-devtools": "3.4.1",
    "redux-devtools-dock-monitor": "1.1.3",
    "redux-devtools-log-monitor": "1.4.0",
    "redux-mock-store": "1.5.3",
    "redux-saga": "0.16.0",
    "redux-storage": "4.1.2",
    "redux-storage-decorator-debounce": "1.1.3",
    "redux-storage-decorator-filter": "1.1.8",
    "redux-storage-decorator-migrate": "1.1.0",
    "reselect": "3.0.1",
    "resize-observer-polyfill": "1.5.0",
    "resolve": "1.6.0",
    "sass-loader": "7.0.0",
    "slug": "0.9.1",
    "style-loader": "0.19.1",
    "styled-components": "3.2.6",
    "svgr": "1.8.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "thread-loader": "1.1.2",
    "uglifyjs-webpack-plugin": "1.1.6",
    "url-loader": "0.6.2",
    "uuid": "3.3.2",
    "valid-url": "1.0.9",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.0",
    "webpack-manifest-plugin": "1.3.2",
    "xterm": "3.12.0"
  },
  "browserslist": "chrome 69",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/**/*.stories.js",
      "!src/components/**/index.js"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/electron.js",
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/enzyme-setup.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/testFrameworkSetup.js",
    "testMatch": [
      "**/__tests__/**/*.{js,jsx,mjs}",
      "**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.(graphql)$": "<rootDir>/config/jest/graphqlTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json|graphql)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$",
      "^.+\\.module\\.css$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.css\\.gif$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "guppy": {
    "id": "guppy",
    "name": "guppy",
    "type": "create-react-app",
    "color": "#FF6D00",
    "icon": null,
    "createdAt": 1529502079329
  }
}
